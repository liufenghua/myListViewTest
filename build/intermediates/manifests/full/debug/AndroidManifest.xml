<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mylistviewtest"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <uses-permission android:name="com.example.contentprovidertestclient" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.mylistviewtest.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.mylistviewtest.MyListActivity"
            android:label="MyListActivity" >
        </activity>
        <activity
            android:name="com.example.mylistviewtest.ArrayAdapterActivity"
            android:label="ArrayAdapterActivity" >
        </activity>
        <activity
            android:name="com.example.mylistviewtest.SimpleAdapterActivity"
            android:label="SimpleAdapterActivity" >
        </activity>
        <activity
            android:name="com.example.mylistviewtest.MyBaseAdapterActivity"
            android:label="MyBaseAdapterActivity" >
        </activity>
        <activity
            android:name="com.example.mylistviewtest.SimpleSursorAdapterActivity"
            android:label="SimpleSursorAdapterActivity" >
        </activity>
    </application>

</manifest>